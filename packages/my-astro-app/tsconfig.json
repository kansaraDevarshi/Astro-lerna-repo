{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
    }
  },
  "exclude": ["node_modules"],
  "include": ["./src/**/*"]
}